{
	"manifest_version": 3,
	"name": "ClearText2",
	"version": "0.1",
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"permissions": ["activeTab"],
	"action": {
	  "default_popup": "popup.html"
	},
	"content_scripts": [
	  {
		"matches": ["<all_urls>"],
		"js": ["content.js"]
	  }
	]
  }